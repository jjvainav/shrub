{
    "name": "@app/workbench",
    "version": "1.0.0",
    "private": true,
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "scripts": {
        "build": "concurrently \"tsc\" \"yarn run copy-styles\" \"yarn run copy-vue\"",
        "clean": "rimraf ./dist && rimraf tsconfig.tsbuildinfo",
        "copy-styles": "copyfiles -u 1 ./src/styles/**/*.scss dist",
        "copy-vue": "copyfiles -u 1 ./src/components/**/*.vue dist"
    },
    "dependencies": {
        "@sprig/event-emitter": "^0.2.0",
        "@sprig/event-emitter-dom": "^0.2.0",
        "vue": "^3.2.20",
        "vue-router": "^4.0.12"
    },
    "devDependencies": {
        "copyfiles": "^2.1.1",
        "concurrently": "^4.1.2"
    }
}