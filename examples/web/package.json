{
    "name": "@examples/web",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "yarn run copy-html && yarn run build-server && yarn run build-app",
        "build-app": "webpack --config webpack.config.js --mode=development",
        "build-server": "tsc && yarn run copy-html",
        "build-start": "yarn run build && yarn run start",
        "clean": "rimraf ./dist && rimraf tsconfig.tsbuildinfo",
        "copy-html": "copyfiles -u 2 ./src/public/**/*.html dist/public",
        "start": "node dist/server.js"
    },
    "dependencies": {
        "@shrub/module": "^0.1.3",
        "@shrub/service-collection": "^0.1.3",
        "connect-history-api-fallback": "^1.6.0",
        "express": "^4.16.4"
    },
    "devDependencies": {
        "@types/connect-history-api-fallback": "^1.3.1",
        "@types/express": "^4.16.1",
        "copyfiles": "^2.1.0",
        "css-loader": "^2.1.1",
        "node-sass": "^4.11.0",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "url-loader": "^1.1.2",
        "vue": "^2.6.8",
        "vue-loader": "^15.7.0",
        "vue-style-loader": "^4.1.2",
        "vue-template-compiler": "^2.6.8",
        "webpack": "^4.28.4",
        "webpack-cli": "^3.2.1"
    },
    "peerDependencies": {
        "@app/demo-flex": "1.0.0",
        "@app/demo-grid": "1.0.0",
        "@app/demo-module": "1.0.0",
        "@app/demo-stack": "1.0.0",
        "@app/workbench-browser": "1.0.0",
        "@shrub/common": "1.0.0",
        "@shrub/express-cookie-session": "1.0.0",
        "@shrub/express-core": "1.0.0",
        "@shrub/express-identity": "1.0.0",
        "@shrub/express-request-tracing": "1.0.0",
        "@shrub/module": "1.0.0",
        "@shrub/random-string": "1.0.0",
        "@shrub/request-client": "1.0.0",
        "@shrub/request-client-oauth": "1.0.0",
        "@shrub/tracing": "1.0.0",
        "@shrub/tracing-console": "1.0.0",
        "@shrub/vue-browser": "1.0.0"
    }
}