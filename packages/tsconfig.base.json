{
    "compilerOptions": {
        "baseUrl": ".",
        "composite": true,
        "declaration": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noEmitOnError": true,
        "inlineSourceMap": true,
        "strict": true,
        "stripInternal": true,
        "moduleResolution": "node",
        "module": "commonjs",
        "target": "esnext",
        "lib": ["dom", "esnext"],
        "paths": {
            "express": ["../../node_modules/express"],
            "http": ["../../node_modules/@types/node/http"],
            "socket.io": ["../../node_modules/@types/socket.io"],
            "vue": ["../../node_modules/vue"]
        }
    }
}