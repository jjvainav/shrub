{
    "compilerOptions": {
        "baseUrl": ".",
        "composite": true,
        "declaration": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noEmitOnError": true,
        "inlineSourceMap": true,
        "strict": true,
        "moduleResolution": "node",
        "module": "commonjs",
        "target": "esnext",
        "lib": ["esnext"],
        "paths": {
            "express": ["../../node_modules/express"],
            "vue": ["../../node_modules/vue"]
        },
    }
}