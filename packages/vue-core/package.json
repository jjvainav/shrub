{
    "name": "@shrub/vue-core",
    "description": "Abstract core module for vue component support and modules should not depend on this directly but instead depend on one of the platform specific modules (e.g. vue-browser, vue-server).",
    "version": "0.1.4",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/jjvainav/shrub.git"
    },
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "tsc -b .",
        "clean": "rimraf ./dist && rimraf tsconfig.tsbuildinfo",
        "watch": "tsc -b . -w"
    },
    "dependencies": {
        "@shrub/module": "0.1.4",
        "@shrub/serialization": "0.1.4",
        "@shrub/service-collection": "0.1.4",
        "vue": "^2.6.10"
    }
}
