{
    "compilerOptions": {
        "baseUrl": ".",
        "composite": true,
        "declaration": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noEmitOnError": true,
        "inlineSourceMap": true,
        "strict": true,
        "moduleResolution": "node",
        "module": "esnext",
        "target": "esnext",
        "lib": [
            "dom",
            "esnext"
        ]
    }
}